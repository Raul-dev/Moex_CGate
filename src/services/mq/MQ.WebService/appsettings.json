{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgresdb;Port=54321;Database=webdwh;Username=postgres;Password=postgres"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "Microsoft.AspNetCore": "Debug",
        "System": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": {
            "type": "Serilog.Templates.ExpressionTemplate, Serilog.Expressions",
            "template": "[{@t:HH:mm:ss.ff}] [{@l:u3}] {@m}\n{@x}"
          }
        }
      },

      {
        "Name": "File",
        "Args": {
          "path": "../logs/MQ-.log",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": "128000",
          "retainedFileCountLimit": 3,
          "formatter": {
            "type": "Serilog.Templates.ExpressionTemplate, Serilog.Expressions",
            "template": "{ {Timestamp: @t, Level: @l, Message: @m, Properties: @p} }\n"
            //"template": "[{@t:HH:mm:ss.ff}] [{@l:u3}] {@m} Context:{#if SourceContext is not null} [{SourceContext}]{#end}\n{@x}"
          }

        }
      }
    ],
    "Properties": {
      "Application": "MQ"
    }
  },
  "DataBaseSettings": {
    "ServerName": "host.docker.internal",
    "Port": "54321",
    "ServerType": "mssql",
    "DataBase": "CGate",
    "ClientName": "CGate",
    "User": "sa",
    "Password": "MyPassword",
    "SessionMode": "FullMode"
  },

  "RabbitMQSettings": {
    "Host": "host.docker.internal",
    "VirtualHost": "/",
    "Exchange": "amq.fanout",
    "Port": 5672,
    "UserName": "admin",
    "UserPassword": "admin",
    "DefaultQueue": "Cgate_FORTS_TRADE_REPL",
    "SslEnabled": "false",
    "SslServerName": "localhost",
    "SslVersion": "3072"
  },
  "KafkaSettings": {
    "Host": "localhost",
    "Port": 9092,
    "Topic": "Cgate_FORTS_TRADE_REPL",
    "GroupId": "somegroup3"
  }
}
